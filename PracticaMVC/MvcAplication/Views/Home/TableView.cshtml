@model List<MvcAplication.Models.MuseoViewModel>

<div class="container">
    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Nombre</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td scope="row">@item.Id</td>
                        <td scope="row">@item.Nombre</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
